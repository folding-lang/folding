package samples

[+] (a~Int b~Int) foreign Int `a + b`
[-] (a~Int b~Int) foreign Int `a - b`
[*] (a~Int b~Int) foreign Int `a * b`
[/] (a~Int b~Int) foreign Double `a / b`

class Console {
    log (msg~String) Unit
}

[|>] T R (value~T func~ (T) -> R) R = #func[#value]

console foreign Console `console`

myFunc T ~ Calc(T) (x~T) T = (#x + 2) + (#x * 7)
inverse r (T (#r - 2)/8)

main Unit = do {
    console:log(-6 |> ::myFunc)
}